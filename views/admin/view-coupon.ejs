<section class="content-main">
    <div class="content-header">
        <div>
            <h2 class="content-title card-title">Coupons</h2>
           
        </div>

    </div>
    <div class="card mb-4">

        <div class="card-body">
            <article class="itemlist">
                <div class="row align-items-center">

                    <div class="col-lg-4 col-sm-2 col-8  col-name">
                        <a class="itemside" href="#">

                            <div class="info">
                                <h4 class="mb-0">Coupon</h4>
                            </div>
                        </a>
                    </div>
                    <div class="col-lg-4 col-sm-2 col-4 col-price"> <h4>Description</h4> </div>
                    <div class="col-lg-4 col-sm-2 col-4 col-status">
                        <h4>Expiry</h4>
                    </div>

                </div> 
            </article>
           <% data.forEach((coupon)=>{ %>

          
            <article class="itemlist">
                <div class="row align-items-center">

                    <div class="col-lg-4 col-sm-2 col-8 flex-grow-1 col-name">
                        <a class="itemside" href="#">

                            <div class="info">
                                <h6 class="mb-0"><%= coupon.code -%></h6>
                            </div>
                        </a>
                    </div>
                    <div class="col-lg-4 col-sm-2 col-4 col-price"> <span><%= coupon.description -%></span> </div>
                    <div class="col-lg-2 col-sm-2 col-4 col-status">
                        <span><%= coupon.expirationDate -%></span>
                    </div>

                    <div class="col-lg-2 col-sm-2 col-4 col-action text-end">
                        <a href="/admin/edit-coupon/<%=coupon._id %>" class="btn btn-sm font-sm rounded btn-brand">
                            <i class="material-icons md-edit"></i> Edit
                        </a>
                        <a href="/admin/delete-coupon/<%=coupon._id %>" class="btn btn-sm font-sm btn-light rounded">
                            <i class="material-icons md-delete_forever"></i> Delete
                        </a>
                    </div>
                </div> 
            </article>
      <%  }) %>
          
        </div>
    </div> 
    <div class="pagination-area mt-30 mb-50">
        <nav aria-label="Page navigation example">
            <ul class="pagination justify-content-start">
                <li class="page-item active"><a class="page-link" href="#">1</a></li>     
            </ul>
        </nav>
    </div>
</section>